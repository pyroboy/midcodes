{
	"name": "id-gen",
	"version": "2",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"serve:edge": "supabase functions serve role-emulation --env-file .env.local",
		"deploy:edge": "supabase functions deploy role-emulation",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"test:credits": "pnpm exec vitest --run tests/unit/credit-*.test.ts",
		"test:credits:watch": "pnpm exec vitest tests/unit/credit-*.test.ts",
		"test:credits:coverage": "pnpm exec vitest --run --coverage tests/unit/credit-*.test.ts",
		"test:credits:templates": "pnpm exec vitest --run tests/unit/credit-template.test.ts",
		"test:credits:generation": "pnpm exec vitest --run tests/unit/credit-card-generation.test.ts",
		"test:credits:insufficient": "pnpm exec vitest --run tests/unit/credit-insufficient.test.ts",
		"test:credits:transactions": "pnpm exec vitest --run tests/unit/credit-transactions.test.ts",
		"test:credits:balance": "pnpm exec vitest --run tests/unit/credit-balance-verification.test.ts",
		"test:credits:bypasses": "pnpm exec vitest --run tests/unit/credit-bypasses.test.ts",
		"test:credits:users": "pnpm exec vitest --run tests/unit/credit-user-creation.test.ts",
		"test:credits:isolation": "pnpm exec vitest --run tests/unit/credit-organization-isolation.test.ts",
		"test:credits:roles": "pnpm exec vitest --run tests/unit/credit-role-permissions.test.ts",
		"clean": "rm -rf .svelte-kit build node_modules/.cache/turbo"
	},
	"devDependencies": {
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-vercel": "^5.5.0",
		"@sveltejs/kit": "^2.27.3",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/vite": "^4.1.12",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.6",
		"@types/eslint": "^8.56.0",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.10.1",
		"@types/webfontloader": "^1.6.38",
		"bits-ui": "^2.9.3",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.6.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.45.1",
		"jest-image-snapshot": "^6.4.0",
		"jsdom": "^25.0.1",
		"lucide-svelte": "^0.461.0",
		"msw": "^2.7.0",
		"postcss": "^8.4.40",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"shadcn-svelte": "^0.13.0",
		"svelte": "^5.2.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^4.1.12",
		"three": "^0.171.0",
		"vite": "^5.4.11",
		"vitest": "^1.0.0",
		"zod": "^4.0.17"
	},
	"dependencies": {
		"@faker-js/faker": "^9.3.0",
		"@internationalized/date": "^3.6.0",
		"@lucide/svelte": "^0.539.0",
		"@shadcn/ui": "^0.0.4",
		"@supabase/ssr": "^0.5.2",
		"@supabase/supabase-js": "^2.46.1",
		"@tanstack/table-core": "^8.21.3",
		"@threlte/core": "^8.0.0-next.41",
		"@threlte/extras": "^9.0.0-next.54",
		"cmdk-sv": "^0.0.19",
		"date-fns": "^4.1.0",
		"esm-env": "^1.2.1",
		"formsnap": "^2.0.1",
		"html5-qrcode": "^2.3.8",
		"jszip": "^3.10.1",
		"jwt-decode": "^4.0.0",
		"lodash-es": "^4.17.21",
		"paneforge": "^1.0.2",
		"svelte-radix": "^1.1.1",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.19.1",
		"tw-animate-css": "^1.3.7",
		"vaul-svelte": "^0.3.2",
		"webfontloader": "^1.6.28"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
